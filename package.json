{
  "name": "heroku-cra-node",
  "version": "3.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "npm run build-server && node ./server/build/index.js",
    "clean": "rimraf ./server/build",
    "build-babel": "babel -d ./server/build ./server/src -s",
    "build-server": "npm run clean && npm run build-babel",
    "build": "cd react-ui/ && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/google-map-react": "^1.1.5",
    "@types/rc-slider": "^8.6.5",
    "@types/react-lottie": "^1.2.3",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.1",
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.3",
    "express-session": "^1.17.0",
    "firebase": "^7.5.2",
    "lowdb": "^1.0.0",
    "mongoose": "^5.7.14",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "node-geocoder": "^3.25.0",
    "nodemon": "^2.0.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "rimraf": "^3.0.0",
    "shortid": "^2.2.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
